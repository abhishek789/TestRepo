pipeline {
    agent any
    tools {
maven  'Maven_3.5.2'
}
    stages {
        stage('Compile stage') {
            steps {
                
                    bat "cd gs-maven-master/complete/pom.xml && mvn clean compile"
            }
        }
    
    
         stage('testing stage') {
             steps {
                 
                    bat "cd gs-maven-master/complete/pom.xml && mvn test"
            }
        }
    

          stage('deployment stage') {
              steps {
                  
                    bat "cd gs-maven-master/complete/pom.xml && mvn deploy"
            }
        }
    

  }

}
