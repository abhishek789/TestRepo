pipeline {
    agent any
    stages {
        stage('Compile stage') {
            
                withMaven(maven : 'Maven_3.5.2'){
                    bat "mvn clean compile"
            }
        }
    
    
         stage('testing stage') {
            
                withMaven(maven : 'Maven_3.5.2'){
                    bat "mvn test"
            }
        }
    

          stage('deployment stage') {
            
                withMaven(maven : 'Maven_3.5.2'){
                    bat "mvn deploy"
            }
        }
    

  }

}
