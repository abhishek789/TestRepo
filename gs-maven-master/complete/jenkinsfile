pipeline {
    agent any
    tools {
maven  'Maven_3.5.2'
}
    stages {
        stage('Compile stage') {
            steps {
                
                    bat "cd C:\Program Files (x86)\Jenkins\workspace\Jenkins-pipeline-example\gs-maven-master\complete"
                    bat "mvn clean compile"
            }
        }
    
    
         stage('testing stage') {
             steps {
                 
                    bat "cd C:\Program Files (x86)\Jenkins\workspace\Jenkins-pipeline-example\gs-maven-master\complete"
                    bat "mvn test"
            }
        }
    

          stage('deployment stage') {
              steps {
                  
                    bat "cd C:\Program Files (x86)\Jenkins\workspace\Jenkins-pipeline-example\gs-maven-master\complete"
                    bat "mvn deploy"
            }
        }
    

  }

}
