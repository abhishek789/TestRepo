pipeline {
    agent any
    tools {
maven  'Maven_3.5.2'
}
    stages {
        stage('build and Compile stage') {
            steps {
                
                    
                    bat "mv install"
            }
        }
    
    
         stage('testing stage') {
             steps {
                 
                    
                    bat "mvn test"
            }
        }
    

          stage('deployment stage') {
              steps {
                  

hygieiaArtifactPublishStep artifactDirectory: 'C:\\Jenkins\\workspace\\Jenkins-pipeline-example\\target\\gs-maven-0.1.0.jar', artifactGroup: 'org.springframework', artifactName: '*.jar', artifactVersion: '0.1.0'
                    
                    hygieiaDeployPublishStep applicationName: 'MyApp', artifactDirectory: 'C:\\Jenkins\\workspace\\Jenkins-pipeline-example\\target\\gs-maven-0.1.0.jar', artifactGroup: 'org.springframework', artifactName: '*.jar', artifactVersion: '0.1.0', buildStatus: 'Success', environmentName: 'Dev


            }
        }
    

  }

}
